{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@firebase/auth": [
				"./node_modules/@firebase/auth/dist/index.rn.d.ts"
			],
			"@screens/*": ["./screens/*"],
			"@components/*": ["./components/*"],
			"@config/*": ["./config/*"],
			"@model/*": ["./models/*"],
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
